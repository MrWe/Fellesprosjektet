<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<?import java.net.*?>
<?import java.lang.*?>

<AnchorPane prefHeight="45.0" prefWidth="1280.0" styleClass="thebackground" stylesheets="@../../bin/styles/toolbarStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ToolbarController">
	<children>
		<HBox prefHeight="45.0" prefWidth="1280.0" spacing="5.0">
			<children>
				<Button cancelButton="true" contentDisplay="top" focusTraversable="false" onAction="#openHome" prefHeight="45.0" prefWidth="45.0" text="Home">
					<graphic>
						<ImageView fitHeight="42" fitWidth="42" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="Images/home_icon.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>

				<Button cancelButton="true" contentDisplay="top" focusTraversable="false" onAction="#openNotice" prefHeight="45.0" prefWidth="45.0" text="Notice">
					<graphic>
						<ImageView fitHeight="42" fitWidth="42" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="Images/noti_icon.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>

				<Button cancelButton="true" contentDisplay="top" focusTraversable="false" onAction="#openSettings" prefHeight="45.0" prefWidth="45.0" text="Settings">
					<graphic>
						<ImageView fitHeight="42" fitWidth="42" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="Images/setting_icon.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>

				<HBox alignment="TOP_RIGHT" prefHeight="45.0" prefWidth="1077.0">
					<children>
						<Button cancelButton="true" contentDisplay="top" focusTraversable="false" onAction="#logOut" prefHeight="42" prefWidth="42" text="Log out">
							<graphic>
								<ImageView fitHeight="42" fitWidth="42" pickOnBounds="true" preserveRatio="true">
									<image>
										<Image url="Images/logout_icon.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
					</children>
					<HBox.margin>
						<Insets right="5.0" />
					</HBox.margin>
				</HBox>
			</children>
			<padding>
				<Insets left="5.0" top="5.0" />
			</padding>
		</HBox>
		<Separator layoutY="80.0" prefHeight="1.0" prefWidth="1280.0" />
	</children>
</AnchorPane>
